  "data" : {
    "contract" : {
      "type" : "constant",
      "exposed" : true
    },
    "submit" : {
      "type" : "expression",
      "value" : "return {\n    \"save\":function(){\n        var contract = angular.copy($data.contract);\n        contract.c_action = \"SAVE\";\n        contract.c_cancelComments = \"\";\n        contract.c_assigned = \"0\";\n        return contract;\n    },\n    \"rollback\":function(){\n        var contract = angular.copy($data.contract);\n        contract.c_action = \"ROLLBACK\";\n        contract.c_cancelComments = \"\";\n        contract.c_assigned = \"0\";\n        return contract;\n    },\n    \"cancel\":function(){\n        var contract = angular.copy($data.contract);\n        contract.c_action = \"CANCEL\";\n        contract.c_cancelComments = $data.comments;\n        contract.c_assigned = \"0\";\n        return contract;\n    },\n    \"continue\":function(){\n        var contract = angular.copy($data.contract);\n        contract.c_action = \"CONTINUE\";\n        contract.c_cancelComments = \"\";\n        contract.c_assigned = \"0\";\n        return contract;\n    },\n    \"assign\":function(){\n        var contract = angular.copy($data.contract);\n        contract.c_action = \"ASSIGN\";\n        contract.c_cancelComments = \"\";\n        contract.c_assigned = $data.assigned;\n        return contract;\n    }\n}",
      "exposed" : false
    },
    "availableActions" : {
      "type" : "url",
      "value" : "../API/bpm/processParameter/{{processId}}/{{paramName}}",
      "exposed" : false
    },
    "processId" : {
      "type" : "constant",
      "exposed" : true
    },
    "task" : {
      "type" : "constant",
      "exposed" : true
    },
    "paramName" : {
      "type" : "expression",
      "value" : "if($data.task && $data.task.id){\n    return \"actions\"+$data.task.name\n}else{\n    return \"actionsInstantiation\"\n}",
      "exposed" : false
    },
    "assigned" : {
      "type" : "constant",
      "exposed" : false
    },
    "comments" : {
      "type" : "constant",
      "exposed" : false
    },
    "usersOfActor" : {
      "type" : "url",
      "value" : "../API/extension/usersOfActor?actor={{actor}}&processId={{processId}}",
      "exposed" : false
    },
    "actor" : {
      "type" : "expression",
      "value" : "if($data.task && $data.task.name){\n    if(\"complete\" === $data.task.name){\n        return \"Actor1\"\n    }else{\n        var currentActorNum = Number($data.task.name.substring(5))+1;\n        return \"Actor\"+currentActorNum\n    }\n}else{\n    return \"Actor1\"\n}",
      "exposed" : false
    }
  },
  "type" : "fragment"
}